#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    int n, search, i, j, step;
    cout << "Enter number of elements: ";
    cin >> n;
    int arr[n];
    cout << "Enter " << n << " elements (sorted order): ";
    for(i=0;i<n;i++){
        cin >> arr[i];
    }
    cout << "Enter element to be searched: ";
    cin >> search;
    step = sqrt(n);
    for(i=0;i<n;i+=step){
        if(arr[i] >= search){
            for(j=i;j>i-step;j--){
                if(arr[j] == search){
                    cout << "Element found at index " << j << endl;
                    return 0;
                }
            }
            cout << "Element not found" << endl;
            return 0;
        }
    }
}
